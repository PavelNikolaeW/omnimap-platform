# =============================================================================
# OmniMap Platform Configuration
# =============================================================================
# Copy this file to .env and update the values

# -----------------------------------------------------------------------------
# Environment
# -----------------------------------------------------------------------------
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# -----------------------------------------------------------------------------
# PostgreSQL
# -----------------------------------------------------------------------------
POSTGRES_USER=omnimap
POSTGRES_PASSWORD=omnimap_secret
POSTGRES_DB=omnimap

# -----------------------------------------------------------------------------
# RabbitMQ
# -----------------------------------------------------------------------------
RABBITMQ_USER=omnimap
RABBITMQ_PASSWORD=omnimap_secret

# -----------------------------------------------------------------------------
# Security Keys (CHANGE IN PRODUCTION!)
# -----------------------------------------------------------------------------
DJANGO_SECRET_KEY=your-django-secret-key-min-32-characters-here
JWT_SECRET=your-jwt-secret-key-min-32-characters-here

# -----------------------------------------------------------------------------
# LLM Providers
# -----------------------------------------------------------------------------
# OpenAI (or OpenAI-compatible)
OPENAI_API_KEY=
OPENAI_BASE_URL=http://localhost:1234/v1

# Anthropic
ANTHROPIC_API_KEY=
